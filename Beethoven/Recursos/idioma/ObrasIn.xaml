<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <sys:String x:Key="Title">Works1 Opera</sys:String>
    <sys:String x:Key="Text1">"I do not play for</sys:String>
    <sys:String x:Key="Text2">Masses</sys:String>
    <sys:String x:Key="Text3">Oratorios, cantatas, and songs with orchestra</sys:String>
    <sys:String x:Key="Text4">9 symphonies</sys:String>
